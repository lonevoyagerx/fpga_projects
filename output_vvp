#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x57cd74884dd0 .scope module, "tb" "tb" 2 3;
 .timescale -9 -12;
v0x57cd74895220_0 .var "clk", 0 0;
v0x57cd748952f0_0 .net "led", 0 0, v0x57cd74895120_0;  1 drivers
S_0x57cd74884f60 .scope module, "uut" "top" 2 9, 3 1 0, S_0x57cd74884dd0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "led";
v0x57cd74885140_0 .net "clk", 0 0, v0x57cd74895220_0;  1 drivers
v0x57cd74895040_0 .var "counter", 7 0;
v0x57cd74895120_0 .var "led", 0 0;
E_0x57cd748854e0 .event posedge, v0x57cd74885140_0;
    .scope S_0x57cd74884f60;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x57cd74895040_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_0x57cd74884f60;
T_1 ;
    %wait E_0x57cd748854e0;
    %load/vec4 v0x57cd74895040_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x57cd74895040_0, 0;
    %load/vec4 v0x57cd74895040_0;
    %parti/s 1, 2, 3;
    %assign/vec4 v0x57cd74895120_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x57cd74884dd0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cd74895220_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x57cd74884dd0;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x57cd74895220_0;
    %inv;
    %store/vec4 v0x57cd74895220_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x57cd74884dd0;
T_4 ;
    %vpi_call 2 23 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57cd74884dd0 {0 0 0};
    %delay 2000000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "design.v";
